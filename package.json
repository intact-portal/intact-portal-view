{
  "name": "intact-portal",
  "version": "1.0.0",
  "license": "The Apache Software License, Version 2.0",
  "angular-cli": {},
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve --base-href=\"/intact/dev/\"",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-gh-pages": "rm -rf dist && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev --output-path=dist --base-href \"https://intact-portal.github.io/intact-portal-view/\" && sudo ngh",
    "proxy": "ng serve --configuraiton=local --base-href \"/intact/local/\" --proxy-config proxy.conf.json",
    "dev": "ng serve --configuration=dev --base-href \"/intact/dev/\"",
    "build-test": "ng serve --configuration=test --build-optimizer=false --base-href \"/intact/beta/\"",
    "update-types": "npm install; for toCopy in \"datatables.net-fixedheader\" \"typeahead.js\"; do rm -rf src/types/$toCopy; cp -rf node_modules/@types/${toCopy} src/types/; done;"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@iktakahiro/markdown-it-katex": "^4.0.0",
    "angular-archwizard": "5.0.1",
    "angular2-query-builder": "^0.6.2",
    "complexviewer": "2.1.13",
    "core-js": "^2.4.1",
    "datatables.net": "^1.10.19",
    "ebi-framework": "1.3.8",
    "ember-cli-cors": "0.0.2",
    "file-saver": "^2.0.5",
    "intact-network-viewer": "0.9.21",
    "jquery": "^3.6.0",
    "jquery-ui-dist": "^1.12.1",
    "katex": "^0.13.18",
    "markdown-it": "^11.0.1",
    "ng2-file-upload": "^1.4.0",
    "ng5-slider": "^1.2.4",
    "ngx-page-scroll": "^7.0.5",
    "ngx-page-scroll-core": "^7.0.5",
    "ngx-google-analytics": "^8.1.1",
    "npm": "^7.6.3",
    "rxjs": "^6.6.7",
    "tslib": "^1.9.0",
    "typeahead.js": "^0.11.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
