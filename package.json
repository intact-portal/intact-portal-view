  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve --configuration=dev",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-gh-pages": "rm -rf dist && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=test --output-path=dist --base-href \"https://intact-portal.github.io/intact-portal-view/\" && sudo ngh",
    "proxy": "ng serve --configuraiton=local --proxy-config proxy.conf.json",
    "build-test": "ng build --configuration=test",
    "update-types": "npm install; for toCopy in \"datatables.net-fixedheader\" \"typeahead.js\"; do rm -rf src/types/$toCopy; cp -rf node_modules/@types/${toCopy} src/types/; done;"
  },
